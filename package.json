{
  "name": "swagger2ts",
  "version": "1.0.0",
  "description": "swagger2ts",
  "main": "lib/swagger2ts.cjs.js",
  "module": "lib/swagger2ts.esm.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "rollup -c rollup.config.js -w --sourcemap",
    "build": "rollup -c rollup.config.js",
    "lint:eslint": "npx eslint \"src/**/*.{ts,tsx}\" --fix --fix",
    "lint:prettier": "npx prettier . --write",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts}": [
      "pnpm lint:prettier",
      "pnpm lint:eslint",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm precommit",
      "commit-msg": "node ./scripts/commit-msg.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangsongyang/swagger2ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiangsongyang/swagger2ts/issues"
  },
  "homepage": "https://github.com/jiangsongyang/swagger2ts#readme",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/ejs": "^3.1.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.8",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "readline": "^1.3.0",
    "rollup": "^2.75.7",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "picocolors": "^1.0.0"
  }
}
